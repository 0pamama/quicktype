{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["es2015"],
        "allowJs": false,
        "declaration": true,
        "typeRoots": ["../node_modules/@types"],
        "strictNullChecks": true,
        "noImplicitAny": true,
        "outDir": "../dist",
